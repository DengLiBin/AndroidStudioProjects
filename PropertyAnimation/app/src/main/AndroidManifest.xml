<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.denglibin.propertyanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".tween.ShowMain"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".tween.ShowTween"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".tween.AllTween"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".drawableanim.ShowDrawableAnim"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".drawableanim.TestInterpolator"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".drawableanim.ShowExample"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".property.ShowProperty"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".property.DingZhi"
        android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".property.ViewAnimateActivity"
        android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".property.LayoutAnimaActivity"
        android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>
